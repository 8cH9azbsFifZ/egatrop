#!/bin/zsh
. /opt/egatrop/lib/egatrop
PATH=$EBASE/bin:$EBASE/lib:$PATH
LD_PATH=$BASE/lib:$LD_PATH
LD_RUN_PATH=$EBASE/lib:$LD_RUN_PATH
LD_LIBRARY_PATH=$EBASE/lib:$LD_LIBRARY_PATH
MANPATH=$EBASE/man:$MANPATH
INCPATH=$EBASE/include:$INCPATH
PYTHON_LIB=$EBASE/lib/python2.5/site-packages:$PYTHON_LIB
PYTHONPATH=$EBASE/lib/python2.5/site-packages:$PYTHONPATH
LAPACK=$BASE/lib/libfblas.a
BLAS=$EBASE/lib/libfblas.a

export PATH LD_PATH LD_RUN_PATH LD_LIBRARY_PATH MANPATH INCPATH PYTHON_LIB PYTHONPATH LAPACK BLAS
